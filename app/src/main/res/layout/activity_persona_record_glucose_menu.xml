<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/green_back"
    tools:context=".updateRecord.personalRecord.Glucose.personal_record_glucose_menu">

    <ImageView
        android:id="@+id/imageGlucose_add"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="30dp"
        android:onClick="sendUserToGlucoseAdd"
        app:layout_constraintEnd_toStartOf="@+id/imageGlucose_View"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/hospital_record_menu_add" />

    <ImageView
        android:id="@+id/imageGlucose_View"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="30dp"
        android:onClick="sendUserToGlucoseView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageGlucose_add"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/hospital_record_menu_view" />
</androidx.constraintlayout.widget.ConstraintLayout>